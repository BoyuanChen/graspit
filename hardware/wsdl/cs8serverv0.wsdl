     <SOAP:header use="literal" message="tns:CS8ServerV0Header" part="sessionId"/>
  </input>
  <output>
     <SOAP:body parts="parameters" use="literal"/>
  </output>
  <fault name="ServerException">
   <SOAP:fault name="ServerException" use="literal"/>
  </fault>
 </operation>
