 <part name="parameters" element="SCSV0:getCS8VersionsResponse"/>
</message>

<message name="login">
 <part name="parameters" element="SCSV0:login"/>
</message>

<message name="loginResponse">
 <part name="parameters" element="SCSV0:loginResponse"/>
</message>

<message name="logout">
 <part name="parameters" element="SCSV0:logout"/>
</message>

<message name="logoutResponse">
 <part name="parameters" element="SCSV0:logoutResponse"/>
</message>

<message name="getRobotsRequest">
 <part name="parameters" element="SCSV0:getRobots"/>
</message>

<message name="getRobotsResponse">
 <part name="parameters" element="SCSV0:getRobotsResponse"/>
</message>

<message name="getRobotJointPos">
 <part name="parameters" element="SCSV0:getRobotJointPos"/>
</message>

<message name="getRobotJointPosResponse">
 <part name="parameters" element="SCSV0:getRobotJointPosResponse"/>
</message>

<message name="getRobotJntCartPos">
 <part name="parameters" element="SCSV0:getRobotJntCartPos"/>
</message>

<message name="getRobotJntCartPosResponse">
 <part name="parameters" element="SCSV0:getRobotJntCartPosResponse"/>
</message>

<message name="setRobotJointPos">
 <part name="parameters" element="SCSV0:setRobotJointPos"/>
</message>

<message name="setRobotPosResponse">
 <part name="parameters" element="SCSV0:setRobotPosResponse"/>
</message>

<message name="CS8ServerV0Header">
 <part name="sessionId" element="SCSV0:sessionId"/>
</message>

<message name="ServerExceptionFault">
 <part name="fault" element="SCSV0:ServerException"/>
</message>

<portType name="CS8ServerV0PortType">
 <operation name="getSoapServerVersion">
  <documentation>get the version of the soap server and give the name and the versio of the client</documentation>
  <input message="tns:getSoapServerVersion"/>
  <output message="tns:getSoapServerVersionResponse"/>
  <fault name="ServerException" message="tns:ServerExceptionFault"/>
 </operation>
 <operation name="ping">
  <documentation>test the memory allocation of the soap</documentation>
  <input message="tns:ping"/>
  <output message="tns:pingResponse"/>
  <fault name="ServerException" message="tns:ServerExceptionFault"/>
 </operation>
 <operation name="getCS8Versions">
  <documentation>get the versions of a controller</documentation>
  <input message="tns:getCS8VersionsRequest"/>
  <output message="tns:getCS8VersionsResponse"/>
  <fault name="ServerException" message="tns:ServerExceptionFault"/>
 </operation>
 <operation name="login">
  <documentation>logs a remote user and returns a session id</documentation>
  <input message="tns:login"/>
  <output message="tns:loginResponse"/>
  <fault name="ServerException" message="tns:ServerExceptionFault"/>
 </operation>
 <operation name="logout">
  <documentation>logs out a user</documentation>
  <input message="tns:logout"/>
  <output message="tns:logoutResponse"/>
  <fault name="ServerException" message="tns:ServerExceptionFault"/>
 </operation>
 <operation name="getRobots">
  <documentation>get an array of robots</documentation>
  <input message="tns:getRobotsRequest"/>
  <output message="tns:getRobotsResponse"/>
  <fault name="ServerException" message="tns:ServerExceptionFault"/>
 </operation>
 <operation name="getRobotJointPos">
  <documentation>get the position of a robot</documentation>
  <input message="tns:getRobotJointPos"/>
  <output message="tns:getRobotJointPosResponse"/>
  <fault name="ServerException" message="tns:ServerExceptionFault"/>
 </operation>
 <operation name="getRobotJntCartPos">
  <documentation>get the joint and cartesian position of a robot with a specified tool and frame</documentation>
  <input message="tns:getRobotJntCartPos"/>
  <output message="tns:getRobotJntCartPosResponse"/>
  <fault name="ServerException" message="tns:ServerExceptionFault"/>
 </operation>
 <operation name="setRobotJointPos">
  <documentation>set the joint position of a robot</documentation>
  <input message="tns:setRobotJointPos"/>
  <output message="tns:setRobotPosResponse"/>
 </operation>
</portType>

