<?xml version="1.0" ?>
<robot type="Snake">
  <palm>sphere.xml</palm>
  <!-- DOF0, snake base rotate -->
  <dof type="r">
    <defaultVelocity>0.0</defaultVelocity>
    <maxEffort>5.0e+7</maxEffort>
    <Kp>1.0e+7</Kp>
    <Kd>1.0e+6</Kd>
    <draggerScale>20.0</draggerScale>
  </dof>
  <dof type="r">
    <!-- DOF1, snake transition between base to bend, fixed -->
    <defaultVelocity>1.0</defaultVelocity>
    <maxEffort>5.0e+7</maxEffort>
    <Kp>1.0e+7</Kp>
    <Kd>1.0e+6</Kd>
    <draggerScale>20.0</draggerScale>
  </dof>
  <dof type="r">
    <!-- DOF2, snake bend -->
    <defaultVelocity>0.0</defaultVelocity>
    <maxEffort>5.0e+7</maxEffort>
    <Kp>1.0e+7</Kp>
    <Kd>1.0e+6</Kd>
    <draggerScale>20.0</draggerScale>
  </dof>
  <dof type="r">
    <!-- DOF3, connection dof from snake 1 to snake 2, fixed -->
    <defaultVelocity>0.0</defaultVelocity>
    <maxEffort>5.0e+7</maxEffort>
    <Kp>1.0e+7</Kp>
    <Kd>1.0e+6</Kd>
    <draggerScale>20.0</draggerScale>
  </dof>
  <!-- DOF4, snake base rotate -->
  <dof type="r">
    <defaultVelocity>0.0</defaultVelocity>
    <maxEffort>5.0e+7</maxEffort>
    <Kp>1.0e+7</Kp>
    <Kd>1.0e+6</Kd>
    <draggerScale>20.0</draggerScale>
  </dof>
  <!-- DOF5, snake2 transition from base rotate to bend, fixed -->
  <dof type="r">
    <defaultVelocity>0.0</defaultVelocity>
    <maxEffort>5.0e+7</maxEffort>
    <Kp>1.0e+7</Kp>
    <Kd>1.0e+6</Kd>
    <draggerScale>20.0</draggerScale>
  </dof>
  <!-- DOF6, snake2 bend -->
  <dof type="r">
    <defaultVelocity>0.0</defaultVelocity>
    <maxEffort>5.0e+7</maxEffort>
    <Kp>1.0e+7</Kp>
    <Kd>1.0e+6</Kd>
    <draggerScale>20.0</draggerScale>
  </dof>
  <chain>
    <transform>
      <translation>0 0 0</translation>
      <rotation>0 y</rotation>
    </transform>
    <joint type="Revolute">
      <theta>d0-90</theta>
      <d>0</d>
      <a>0</a>
      <alpha>-90</alpha>
      <minValue>-360</minValue>
      <maxValue>360</maxValue>
    </joint>
    <joint type="Revolute">
      <theta>d1+90</theta>
      <d>0</d>
      <a>0</a>
      <alpha>90</alpha>
      <minValue>0</minValue>
      <maxValue>0</maxValue>
    </joint>
    <joint type="Snake">
      <theta>d2</theta>
      <d>0</d>
      <a>50</a>
      <alpha>0</alpha>
      <minValue>-60</minValue>
      <maxValue>60</maxValue>
      <snakeLength>200</snakeLength>
      <snakeLinks>4</snakeLinks>
    </joint>
    <joint type="Snake">
      <theta>d2</theta>
      <d>0</d>
      <a>50</a>
      <alpha>0</alpha>
      <minValue>-60</minValue>
      <maxValue>60</maxValue>
