      <theta>d5+90</theta>
      <d>0</d>
      <a>0</a>
      <alpha>90</alpha>
      <minValue>0</minValue>
      <maxValue>0</maxValue>
    </joint>
    <joint type="Snake">
      <theta>d6</theta>
      <d>0</d>
      <a>50</a>
      <alpha>0</alpha>
      <minValue>-60</minValue>
      <maxValue>60</maxValue>
      <snakeLength>150</snakeLength>
      <snakeLinks>3</snakeLinks>
    </joint>
    <joint type="Snake">
      <theta>d6</theta>
      <d>0</d>
      <a>50</a>
      <alpha>0</alpha>
      <minValue>-60</minValue>
      <maxValue>60</maxValue>
      <snakeLength>150</snakeLength>
      <snakeLinks>3</snakeLinks>
    </joint>
    <joint type="Snake">
