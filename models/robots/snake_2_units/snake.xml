      <a>50</a>
      <alpha>0</alpha>
      <minValue>-60</minValue>
      <maxValue>60</maxValue>
      <snakeLength>200</snakeLength>
      <snakeLinks>4</snakeLinks>
    </joint>
    <joint type="Snake">
      <theta>d2</theta>
      <d>0</d>
      <a>50</a>
      <alpha>0</alpha>
      <minValue>-60</minValue>
      <maxValue>60</maxValue>
      <snakeLength>200</snakeLength>
      <snakeLinks>4</snakeLinks>
    </joint>
    <!-- a connection between the snake1 to the base of snake2, fixed -->
    <joint type="Revolute">
      <theta>d3+-90</theta>
      <d>0</d>
      <a>0</a>
      <alpha>90</alpha>
      <minValue>0</minValue>
      <maxValue>0</maxValue>
    </joint>
    <!-- snake 2 base rotation -->
