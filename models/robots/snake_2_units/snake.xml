      <a>50</a>
      <alpha>0</alpha>
      <minValue>-60</minValue>
      <maxValue>60</maxValue>
      <snakeLength>150</snakeLength>
      <snakeLinks>3</snakeLinks>
    </joint>
    <!-- snake 1 -->
    <link dynamicJointType="Revolute" collisionRule="Off">faux.xml</link>
    <link dynamicJointType="Revolute" collisionRule="Off">faux.xml</link>
    <link dynamicJointType="Revolute" collisionRule="Off">faux.xml</link>
    <link dynamicJointType="Revolute">snakeunit.xml</link>
    <link dynamicJointType="Revolute" collisionRule="Off">faux.xml</link>
    <link dynamicJointType="Revolute">snakeunit.xml</link>
    <link dynamicJointType="Revolute" collisionRule="Off">faux.xml</link>
    <link dynamicJointType="Revolute">snakeunit.xml</link>
    <link dynamicJointType="Revolute" collisionRule="Off">faux.xml</link>
    <link dynamicJointType="Revolute">snakeunit.xml</link>
    <!-- connection between snake 1 and 2-->
